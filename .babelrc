{
  "presets": ["next/babel"],
  "plugins": [
    ["@babel/plugin-proposal-nullish-coalescing-operator"],
    "babel-plugin-transform-typescript-metadata",
    [
      "@babel/plugin-proposal-decorators",
      {
        "legacy": true
      }
    ],
    [
      "module-resolver",
      {
        "root": ["."],
        "alias": {
          "client": "./client",
          "server": "./server",
          "styles": "./styles"
        }
      }
    ]
  ]
}
