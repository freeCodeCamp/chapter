{
  "extends": ["config:base"],
  "prCreation": "not-pending",
  "labels": ["renovate"],
  "packageRules": [
    {
      "matchUpdateTypes": ["minor", "patch", "pin", "digest"],
      "matchCurrentVersion": "!/^0/",
      "automerge": true,
      "automergeType": "branch"
    }
  ],
  "rangeStrategy": "pin"
}
